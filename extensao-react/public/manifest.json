{
    "name": "JetSales",
    "description": "Chrome Extension",
    "version": "1.1",
    "manifest_version": 3,
    "permissions": [
    "scripting", 
    "activeTab",
     "sidePanel"

  ],
"action": {
    "default_title": "Click to open panel"
  },
"side_panel": {
    "default_path": "index.html"
  },
"background": {
    "service_worker": "background.js"

  }
}